  "repository": {
    "type": "git",
    "url": "https://github.com/zslucky/redux-fetch-middleware.git"
  },
  "keywords": [
    "redux",
    "fetch",
    "middleware",
    "rest",
    "cache",
    "pagination",
    "loading"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zslucky/redux-fetch-middleware/issues"
  },
  "homepage": "https://github.com/zslucky/redux-fetch-middleware",
  "dependencies": {
    "lodash.merge": "^4.6.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^4.1.2",
    "freelint": "3.1.1",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^4.0.1",
    "nock": "^9.1.2",
    "sinon-chai": "^2.14.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  }
}
