{
  "name": "redux-fetch-middleware",
  "version": "4.0.2",
  "description": "A middleware for redux that help to fetch data from rest API",
  "main": "dist/index.js",
  "scripts": {
    "lint": "standard | snazzy",
    "test": "mocha --compilers js:babel-core/register ./test/*.js",
    "build": "npm run lint && npm run test && babel src --out-dir dist"
  },
  "standard": {
    "ignore": [
      "dist/**"
    ],
    "globals": [
      "describe",
      "it",
      "before",
      "fetch"
    ]
